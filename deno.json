{
  "tasks": {
    "start": "deno run --allow-all src/main.ts",
    "reload": "deno run --reload src/main.ts",
    "dev": "deno run --watch src/main.ts",
    "test": "deno test --allow-all test/**/*.test.ts",
    "coverage": "deno test --allow-all --coverage=cov"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/path": "jsr:@std/path@^1.1.3",
    "@zaubrik/djwt": "jsr:@zaubrik/djwt@^3.0.2",
    "get-port-please": "npm:get-port-please@^3.2.0"
  }
}
