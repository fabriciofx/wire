{
  "tasks": {
    "start": "deno run --allow-all src/main.ts",
    "reload": "deno run --reload src/main.ts",
    "dev": "deno run --watch src/main.ts",
    "test": "deno test --allow-all test/**/*.test.ts",
    "coverage": "deno test --allow-all --coverage=cov"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "std/dotenv": "https://deno.land/std@0.224.0/dotenv/mod.ts"
  }
}
